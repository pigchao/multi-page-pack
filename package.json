{
  "name": "multi-page-pack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build-lib",
    "tslint:fix": "tslint --fix -c ./tslint.json ./src/**/*{.ts,.tsx}",
    "prettier": "prettier --write ./src/**/*{.ts,.tsx}",
    "start": "cross-env NET_ENV=development TS_NODE_PROJECT=tsconfig-server.json node -r ts-node/register src/server/devServer",
    "debug": "cross-env NET_ENV=development TS_NODE_PROJECT=tsconfig-server.json node --inspect -r ts-node/register src/server/devServer",
    "build-lib": "webpack -p --progress --profile --colors --config webpack.dll.config.js",
    "build-client": "cross-env NET_ENV=production STATIC_PATH=cdn webpack --progress --profile --colors --config webpack.base.js",
    "build-client-local": "cross-env NET_ENV=production webpack -p --progress --profile --colors --config webpack.base.js",
    "build-client-prev": "cross-env NET_ENV=preview webpack -p --progress --profile --colors --config webpack.base.js",
    "build-server-prev": "cross-env NET_ENV=preview webpack --progress --profile --colors --config webpack.base.server.js",
    "upload-oss": "node uploadAliOSS",
    "build-prev": "npm install && npm run build-server-prev && npm run build-client-prev",
    "build-server": "cross-env NET_ENV=production webpack --progress --profile --colors --config webpack.base.server.js",
    "build": "npm install && npm run build-server && npm run build-client ",
    "debug-prev": "npm run build-prev && node --inspect ./dist/server/server.js",
    "reload-prev": "cross-env NET_ENV=preview pm2 reload pm2-dev.json",
    "restart-prev": "pm2 delete pm2-dev.json && cross-env NET_ENV=preview pm2 start pm2-dev.json",
    "reload": "cross-env NET_ENV=production pm2 reload pm2.json",
    "restart": "pm2 delete pm2.json && cross-env NET_ENV=production pm2 start pm2.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tx,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+#"
  },
  "author": "lwch",
  "license": "ISC",
  "homepage": "#",
